{
  "name": "uk-account-check",
  "version": "0.0.0",
  "description": "Uses the Vocalink Modulus Checking system to verify UK bank account details",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown --minify",
    "changeset": "changeset",
    "dev": "tsdown --watch",
    "lint": "biome lint",
    "release": "pnpm build && changeset publish",
    "test:watch": "vitest",
    "test": "vitest run",
    "version": "changeset version"
  },
  "keywords": [
    "modulus",
    "bank",
    "account",
    "vocalink"
  ],
  "author": "Glenn Sayers",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/glennsayers/uk-account-check"
  },
  "bugs": {
    "url": "https://github.com/glennsayers/uk-account-check/issues"
  },
  "homepage": "https://github.com/glennsayers/uk-account-check#readme",
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.5.2",
    "tsdown": "^0.15.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
